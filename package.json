{
  "name": "@evolvouslimited/bsmarqueedemo",
  "version": "1.0.7",
  "description": "button UI for test",
  "main": "src/index.ts",
  "files": [
    "src/**/*"
  ],
  "compilerOptions": {
    "outDir": "lib/esm",
    "module": "esnext",
    "target": "es5",
    "lib": [
      "es6",
      "dom",
      "es2016",
      "es2017"
    ],
    "jsx": "react-jsx",
    "declaration": true,
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "esModuleInterop": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "allowSyntheticDefaultImports": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/evolvouslimited/bsmarqueedemo.git"
  },
  "bugs": {
    "url": "https://github.com/evolvouslimited/bsmarqueedemo/issues"
  },
  "homepage": "https://github.com/evolvouslimited/bsmarqueedemo",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "semantic-release": "semantic-release"
  },
  "author": "evolvous",
  "license": "ISC",
  "dependencies": {
    "@types/jest": "^26.0.24"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "semantic-release": "^18.0.1",
    "typescript": "^4.4.3",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/node": "^12.20.27"
  },
  "publishConfig": {
    "access": "public"
  }
